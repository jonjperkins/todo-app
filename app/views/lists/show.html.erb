<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @list.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @list.description %>
</p>

<div id="items_wrapper">
  <%= render @list.items %>
  <div id="form">
    <%= render "items/form" %>
  </div>
</div>

<%= link_to 'Edit', edit_list_path(@list) %> |
<%= link_to 'Delete', list_path(@list), method: :delete, data: { confirm: "Are you sure?" } %> |
<%= link_to 'Back', lists_path %>
